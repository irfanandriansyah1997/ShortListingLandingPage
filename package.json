    "jest": {
        "collectCoverage": true,
        "testURL": "http://localhost/",
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!.storybook/**/*.{js,jsx,ts,tsx}",
            "!src/app.tsx",
            "!src/components/themes/**/*.{js,jsx,ts,tsx}",
